@page
@model LearnIdentity.Pages.Admin.ClaimsModel
@{
    Layout = "Shared/_AdminLayout";
}

<table class="table table-bordered table-striped table-responsive">
     <tr>
        <th>Kim</th>
        <th>Dağıtıcı</th>
        <th>Ad</th>
        <th>Değer</th>
    </tr>
    @foreach(System.Security.Claims.Claim item in Model.claims)
    {
         <tr>
            <td>@item.Subject.Name</td>
            <td>@item.Issuer</td>
            <td>@item.Type</td>
            <td>@item.Value</td>
        </tr>
    }
</table>
